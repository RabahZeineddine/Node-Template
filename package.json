{
  "name": "node-template",
  "version": "1.0.0",
  "description": "",
  "main": "src/index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "cross-env NODE_ENV=development nodemon -e '*' --exec 'npm run local'",
    "qa": "cross-env NODE_ENV=quality_assurance nodemon --exec 'npm run local'",
    "local": "npm run linter && babel-node src",
    "linter": "eslint src/**",
    "build": "babel src --out-dir dist",
    "start": "cross-env NODE_ENV=production npm run server",
    "server": "npm run build && node dist/index",
    "clean-install": "rm -rf node_modules && npm install"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run linter"
    }
  },
  "author": "Rabah Zeineddine",
  "license": "ISC",
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.0",
    "@babel/node": "^7.8.7",
    "@babel/preset-env": "^7.9.0",
    "babel-eslint": "^10.1.0",
    "nodemon": "^2.0.2",
    "eslint": "^6.8.0"
  },
  "dependencies": {
    "@babel/polyfill": "^7.8.7",
    "@hapi/joi": "^17.1.1",
    "body-parser": "^1.19.0",
    "chalk": "^3.0.0",
    "cors": "^2.8.5",
    "cross-env": "^7.0.2",
    "dotenv": "^8.2.0",
    "errorhandler": "^1.5.1",
    "express": "^4.17.1",
    "express-joi-validation": "^4.0.3",
    "husky": "^4.2.3",
    "joi": "^14.3.1",
    "mongoose": "^5.9.6",
    "morgan": "^1.10.0",
    "request": "^2.88.2",
    "swagger-jsdoc": "^4.0.0",
    "swagger-ui-express": "^4.1.4"
  },
  "engines": {
    "node": ">=6.9",
    "npm": "5.x"
  }
}